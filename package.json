{
  "name": "schwrc",
  "version": "1.0.4",
  "description": "A CLI tool for working with files",
  "main": "dist/index.js",
  "bin": {
    "schwrc": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "cli",
    "shell",
    "alias",
    "function"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.14"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
